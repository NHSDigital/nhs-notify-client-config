{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "dataschema": {
      "type": "string",
      "enum": [
        "https://notify.nhs.uk/events/schemas/config-removed/v1.json"
      ]
    },
    "dataschemaversion": {
      "type": "string",
      "const": "1.0.0"
    },
    "type": {
      "type": "string",
      "const": "uk.nhs.notify.config.ConfigRemoved.v1"
    },
    "id": {
      "type": "string"
    },
    "source": {
      "type": "string"
    },
    "specversion": {
      "type": "string",
      "const": "1.0"
    },
    "subject": {
      "type": "string"
    },
    "time": {
      "type": "string"
    },
    "datacontenttype": {
      "type": "string",
      "const": "application/json"
    },
    "plane": {
      "type": "string",
      "const": "control"
    },
    "data": {
      "type": "object",
      "properties": {
        "configType": {
          "type": "string",
          "enum": [
            "client",
            "channel-supplier",
            "supplier-quota",
            "feature-flag"
          ]
        },
        "id": {
          "type": "string"
        }
      },
      "required": [
        "configType",
        "id"
      ],
      "additionalProperties": false
    }
  },
  "required": [
    "dataschema",
    "dataschemaversion",
    "type",
    "id",
    "source",
    "specversion",
    "subject",
    "time",
    "datacontenttype",
    "plane",
    "data"
  ],
  "additionalProperties": false
}
